{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "__DEV__": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    // "indent": [2, "tab"],
    "comma-dangle": 0,
    "no-console": 0,
    "strict": 0,
    "spaced-comment": 0,
    "new-cap": [2, { "capIsNewExceptions": ["Map"] }],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  },
  "plugins": ["react"]
}